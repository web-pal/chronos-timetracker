[ignore]
<PROJECT_ROOT>/node_modules/*(?!redux-form)
<PROJECT_ROOT>/app/main.prod.js
<PROJECT_ROOT>/app/main.prod.js.map
<PROJECT_ROOT>/app/dist/.*
<PROJECT_ROOT>/resources/.*
<PROJECT_ROOT>/release/.*
<PROJECT_ROOT>/dll/.*
<PROJECT_ROOT>/release/.*
<PROJECT_ROOT>/git/.*

[libs]
flow-typed

[options]
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
esproposal.export_star_as=enable
module.name_mapper.extension='less' -> '<PROJECT_ROOT>/app/assets/stub.js'
suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe
module.name_mapper='^actions$' -> '<PROJECT_ROOT>/app/actions/'
module.name_mapper='^types$' -> '<PROJECT_ROOT>/app/types/'
module.name_mapper='^components$' -> '<PROJECT_ROOT>/app/components/'
module.name_mapper='^selectors$' -> '<PROJECT_ROOT>/app/selectors/'
module.name_mapper='^styles$' -> '<PROJECT_ROOT>/app/styles/'
module.name_mapper='^styles/typography$' -> '<PROJECT_ROOT>/app/styles/typography/'
module.name_mapper='^styles/modals$' -> '<PROJECT_ROOT>/app/styles/modals/'
module.name_mapper='^styles/buttons$' -> '<PROJECT_ROOT>/app/styles/buttons/'

module.name_mapper='^api$' -> '<PROJECT_ROOT>/app/utils/api/'
module.name_mapper='^config$' -> '<PROJECT_ROOT>/app/config/'
module.name_mapper='^data$' -> '<PROJECT_ROOT>/app/utils/data/'
module.name_mapper='^data/svg$' -> '<PROJECT_ROOT>/app/utils/data/svg/'
module.name_mapper='^data/assets$' -> '<PROJECT_ROOT>/app/utils/data/assets/'
module.name_mapper='^external-open-util$' -> '<PROJECT_ROOT>/app/utils/external-open-util/'
module.name_mapper='^jiraColors-util$' -> '<PROJECT_ROOT>/app/utils/jiraColors-util/'
module.name_mapper='^log-util$' -> '<PROJECT_ROOT>/app/utils/log-util/'
module.name_mapper='^screenshot-util$' -> '<PROJECT_ROOT>/app/utils/screenshot-util/'
module.name_mapper='^time-util$' -> '<PROJECT_ROOT>/app/utils/time-util/'
module.name_mapper='^timer-helper$' -> '<PROJECT_ROOT>/app/utils/timer-helper/'
module.name_mapper='^styled-components2$' -> '<PROJECT_ROOT>/app/styled-components.min.js'
