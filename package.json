{
  "name": "Chronos",
  "version": "2.8.6",
  "description": "Full functionality time tracking software with direct JIRA integration",
  "scripts": {
    "dev": "yarn start-renderer-dev",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "ship": "electron-builder -p always",
    "build": "concurrently \"yarn build-main\" \"yarn build-renderer\"",
    "package": "yarn build && electron-builder --dir",
    "build-main": "webpack --env.NODE_ENV=production --env.DEBUG_PROD=true -r \"@babel/register\" --config webpack.main.prod.js --colors --progress --profile",
    "build-renderer": "webpack --env.NODE_ENV=production -r \"@babel/register\" --config  webpack.renderer.prod.js --colors --progress --profile",
    "start-renderer-dev": "webpack-dev-server --env.NODE_ENV=development -r \"@babel/register\" --config webpack.renderer.dev.js --colors",
    "start-main-dev": "electron -r '@babel/register' app/main",
    "postinstall": "electron-builder install-app-deps",
    "commit": "git-cz",
    "release": "build",
    "reinstall": "rm -rf node_modules && rm -rf app/node_modules && rm yarn.lock && yarn"
  },
  "license": "ISC",
  "author": {
    "name": "Webpal",
    "url": "https://github.com/web-pal"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/web-pal/chronos-timetracker.git"
  },
  "build": {
    "appId": "com.web-pal.chronos",
    "productName": "Chronos",
    "copyright": "Copyright Â© 2017 Webpal",
    "compression": "normal",
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "category": "public.app-category.productivity-tool"
    },
    "directories": {
      "output": "release"
    }
  },
  "directories": {
    "app": "./dist/",
    "output": "./release/"
  },
  "devEngines": {
    "node": ">=8.x",
    "npm": ">=5.x",
    "yarn": ">=1.8.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/plugin-external-helpers": "7.0.0",
