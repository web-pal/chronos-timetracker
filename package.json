    "redux-saga": "0.16.2",
    "reselect": "4.0.0",
    "smoothscroll-polyfill": "0.4.3",
    "socket.io-client": "2.1.1",
    "styled-components": "3.4.10",
    "tough-cookie": "^2.4.3"
  },
  "devEngines": {
    "node": ">=10.x",
    "npm": ">=6.x",
    "yarn": ">=1.x"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
