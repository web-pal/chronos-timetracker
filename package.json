    "redux-resource-plugins": "3.1.0",
    "redux-saga": "1.0.2",
    "reselect": "4.0.0",
    "rimraf": "2.6.3",
    "screenshot-desktop": "1.7.0",
    "smoothscroll-polyfill": "0.4.4",
    "styled-components": "3.4.5",
    "svg-inline-loader": "0.8.0"
  }
}
