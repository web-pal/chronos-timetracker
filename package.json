    "@atlaskit/checkbox": "6.0.3",
    "@atlaskit/dropdown-menu": "7.0.4",
    "@atlaskit/field-base": "12.0.1",
    "@atlaskit/field-radio-group": "5.0.2",
    "@atlaskit/field-text": "8.0.2",
    "@atlaskit/field-text-area": "5.0.3",
    "@atlaskit/flag": "10.0.5",
    "@atlaskit/icon": "16.0.8",
    "@atlaskit/inline-dialog": "10.0.3",
    "@atlaskit/inline-edit": "9.0.0",
    "@atlaskit/input": "5.0.1",
    "@atlaskit/lozenge": "7.0.1",
    "@atlaskit/modal-dialog": "8.0.6",
    "@atlaskit/section-message": "2.0.2",
    "@atlaskit/select": "8.1.0",
    "@atlaskit/single-select": "7.0.2",
    "@atlaskit/spinner": "10.0.5",
    "@atlaskit/tag": "7.0.1",
    "@atlaskit/tooltip": "13.0.3",
    "@babel/polyfill": "7.4.3",
    "@babel/runtime": "7.4.3",
    "@hot-loader/react-dom": "16.8.6",
    "@sentry/electron": "0.17.1",
    "cross-env": "5.2.0",
