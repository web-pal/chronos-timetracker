    "electron-positioner": "^4.1.0",
    "font-awesome": "4.7.0",
    "luxon": "1.12.1",
    "merge-images": "1.1.0",
    "mixpanel-browser": "2.27.1",
    "moment": "2.24.0",
    "moment-duration-format": "2.2.2",
    "nanotimer": "Krb686/nanotimer",
    "ramda": "0.26.1",
    "rc-progress": "2.3.0",
    "rc-time-picker": "3.6.4",
    "react": "16.8.6",
    "react-click-outside": "tj/react-click-outside",
